    "import sys\n",
    "import matplotlib.pyplot as plt\n",
    "from gpaw import GPAW\n",
    "\n",
    "calc1 = GPAW('G1.gpw', txt=None)\n",
    "\n",
    "try:\n",
    "    ef1 = calc1.get_fermi_level()\n",
    "except ValueError:\n",
    "    ef1 = 0\n",
    "energy1, dos1 = calc1.get_orbital_ldos(0, spin=0, angular='spdf', width=0.25)\n",
    "\n",
    "en1 = -1. * (energy1 - ef1)\n",
    "\n",
    "Spdos_Li = [0] * len(dos1)\n",
